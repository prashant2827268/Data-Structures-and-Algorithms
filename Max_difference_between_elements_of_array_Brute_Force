class Solution {
    public int maximumDifference(int[] nums) {
        int n = nums.length;
        int maxDifference = 0;
        for(int i = 0; i< n-1; i++){
            for(int j = i+1; j<n; j++)
            {
                if(nums[j] > nums[i] && nums[j] - nums[i] > maxDifference){
                    maxDifference = nums[j] - nums[i];
                }
            }
        }
        if(maxDifference > 0){
            return  maxDifference;
        }
        return -1;
        
    }
}